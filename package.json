{
  "name": "taro-start-plus",
  "version": "1.1.0",
  "private": true,
  "description": "taro start plus",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "taroConfig": {
    "version": "1.0.1",
    "desc": "接入CI"
  },
  "scripts": {
    "prepare": "husky install",
    "build:weapp": "taro build --type weapp",
    "build:weapp:preview": "taro build --type weapp --preview",
    "build:weapp:upload": "taro build --type weapp --upload",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:lark": "taro build --type lark",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "run-p \"build:weapp -- --watch\" \"lint:all\"",
    "dev:swan": "run-p \"build:swan -- --watch\" \"lint:all\"",
    "dev:alipay": "run-p \"build:alipay -- --watch\" \"lint:all\"",
    "dev:tt": "run-p \"build:tt -- --watch\" \"lint:all\"",
    "dev:lark": "run-p \"build:lark -- --watch\" \"lint:all\"",
    "dev:h5": "run-p \"build:h5 -- --watch\" \"lint:all\"",
    "dev:rn": "run-p \"build:rn -- --watch\" \"lint:all\"",
    "dev:qq": "run-p \"build:qq -- --watch\" \"lint:all\"",
    "dev:jd": "run-p \"build:jd -- --watch\" \"lint:all\"",
    "dev:quickapp": "run-p \"build:quickapp -- --watch\" \"lint:all\"",
    "lint": "./node_modules/.bin/eslint --ext .ts,.tsx src",
    "lint:fix": "./node_modules/.bin/eslint --ext .ts,.tsx src/ --fix",
    "lint:watch": "nodemon --watch src --ext .ts,.tsx --exec npm run lint",
    "lintstyle": "stylelint src/**/*.{css,scss}",
    "lintstyle:fix": "stylelint src/**/*.{css,scss} --fix",
    "lintstyle:watch": "nodemon --watch src --ext .css,.scss --exec npm run lintstyle",
    "ls-lint": "ls-lint",
    "ls-lint:watch": "nodemon --watch src --exec npm run ls-lint",
    "lint:all": "run-p lint:watch lintstyle:watch ls-lint:watch",
    "create:page": "node scripts/page",
    "create:comp": "node scripts/component",
    "mock": "mocker ./mock",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "release": "release-it",
    "release:major": "release-it major",
    "release:minor": "release-it minor",
    "release:patch": "release-it patch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@antv/f2": "^4.0.46",
    "@babel/runtime": "^7.7.7",
    "@icon-park/svg": "^1.4.2",
    "@poster-render/shared": "^3.5.3",
    "@poster-render/taro": "^3.6.3",
    "@poster-render/taro-react": "^3.6.3",
    "@tarojs/components": "3.6.13",
    "@tarojs/helper": "3.6.13",
    "@tarojs/plugin-framework-react": "3.6.13",
    "@tarojs/plugin-platform-alipay": "3.6.13",
    "@tarojs/plugin-platform-h5": "3.6.13",
    "@tarojs/plugin-platform-jd": "3.6.13",
    "@tarojs/plugin-platform-lark": "^1.1.1",
    "@tarojs/plugin-platform-qq": "3.6.13",
    "@tarojs/plugin-platform-swan": "3.6.13",
    "@tarojs/plugin-platform-tt": "3.6.13",
    "@tarojs/plugin-platform-weapp": "3.6.13",
    "@tarojs/react": "3.6.13",
    "@tarojs/runtime": "3.6.13",
    "@tarojs/shared": "3.6.13",
    "@tarojs/taro": "3.6.13",
    "classnames": "^2.3.2",
    "lodash-es": "^4.17.21",
    "number-precision": "^1.6.0",
    "process": "^0.11.10",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "svg64": "^2.0.0",
    "taro-code": "^4.0.1",
    "umtrack-alipay": "^2.8.0",
    "umtrack-tt": "^2.8.0",
    "umtrack-wx": "^2.8.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@ls-lint/ls-lint": "^2.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@release-it/conventional-changelog": "^7.0.0",
    "@tarojs/cli": "3.6.13",
    "@tarojs/plugin-mini-ci": "^3.6.13",
    "@tarojs/taro-loader": "3.6.13",
    "@tarojs/webpack5-runner": "3.6.13",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.0",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "babel-preset-taro": "3.6.13",
    "commitizen": "^4.3.0",
    "cz-git": "^1.7.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-taro": "^3.6.13",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "mocker-api": "^2.9.8",
    "mockjs": "^1.1.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "postcss-scss": "^4.0.6",
    "react-refresh": "^0.11.0",
    "release-it": "^16.1.0",
    "stylelint": "^15.10.1",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-standard": "^34.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.1.0",
    "webpack": "5.78.0",
    "yargs": "^17.7.2"
  },
  "lint-staged": {
    "src/**/*.+(ts,tsx)": [
      "npm run lint:fix",
      "git add"
    ],
    "src/**/*.+(css|scss)": [
      "npm run lintstyle:fix",
      "git add"
    ],
    "src/**/*": [
      "npm run ls-lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "engines": {
    "node": ">=16.1.2"
  }
}
